{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
      {
        "type": "prompt",
        "alignment": "left",
        "segments": [
          {
            "type": "path",
            "style": "plain",
            "powerline_symbol": "",
            "properties": {
              "style": "full",
              "enable_hyperlink": false,
              "home_icon": "",
              "mapped_locations": {}
            }
          },
          {
            "type": "git",
            "style": "plain",
            "powerline_symbol": "",
            "properties": {
              "display_status": false,
              "display_stash_count": false,
              "display_upstream_icon": false
            }
          }
        ]
      }
    ],
    "final_space": true
  }